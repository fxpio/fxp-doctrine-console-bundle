{
    "name": "sonatra/doctrine-console-bundle",
    "type": "symfony-bundle",
    "description": "Helper for display and manage the doctrine object in console",
    "keywords": ["doctrine", "console", "helper", "object", "entity", "command", "manipulation"],
    "homepage": "https://github.com/sonatra/sonatra-doctrine-console-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "symfony/framework-bundle": "^3.4.0|^4.0.0",
        "symfony/expression-language": "^3.4.0|^4.0.0",
        "sonatra/doctrine-console": "^1.0.0"
    },
    "require-dev": {
        "sonatra/default-value-bundle": "^1.0.0",
        "sonatra/resource-bundle": "^1.0.0"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\DoctrineConsoleBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
